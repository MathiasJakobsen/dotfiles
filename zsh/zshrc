ZSH=$HOME/.oh-my-zsh
ZSH_THEME=""
LANG=en_US.UTF-8
LC_ALL=en_US.UTF-8

export DISABLE_AUTO_TITLE="true"

plugins=(docker gem coffee tmux tmuxinator brew git battery osx rake ruby rvm mathiasjakobsen zsh-syntax-highlighting)
fpath=(/usr/local/share/zsh-completions $fpath)

source $ZSH/oh-my-zsh.sh
source $HOME/Google\ Drev/Apps\ Sync/Dotfiles/github_token.sh

echo "export SSH_AUTH_SOCK=$SSH_AUTH_SOCK" > ~/.auth_ssh

bindkey -v
bindkey '^R' history-incremental-pattern-search-backward


export PATH="$HOME/bin:$PATH"
export PATH="/usr/local/bin:$PATH:$HOME/.rvm/bin"
export PATH="/usr/local/bin:/usr/local/sbin:$PATH"
export PATH="$PATH:`yarn global bin`"
source `brew --prefix`/etc/profile.d/z.sh


[ -f "$HOME/.travis/travis.sh" ] && source $HOME/.travis/travis.sh
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"

eval $(thefuck --alias)


# added by travis gem
[ -f /Users/mathiasjakobsen/.travis/travis.sh ] && source /Users/mathiasjakobsen/.travis/travis.sh
export PATH="/usr/local/opt/node@6/bin:$PATH"
export PATH="/usr/local/opt/node@6/bin:$PATH"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
source /usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh

autoload -U promptinit; promptinit
prompt pure

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
