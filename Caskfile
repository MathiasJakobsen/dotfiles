#!/bin/bash

brew tap caskroom/cask
brew tap caskroom/versions

apps=(
  1password
  alfred
  caffeine
  caskroom/versions/iterm2-nightly
  chefdk
  dropbox
  firefox
  goofy
  google-backup-and-sync
  google-chrome
  hipchat
  java
  postman
  rescuetime
  sequel-pro
  silverlight
  skype
  spotify
  teamviewer
  vlc
)

brew cask install --appdir="/Applications" ${apps[@]}
brew cleanup
