#!/bin/bash

brew install caskroom/cask/brew-cask
brew tap caskroom/versions

apps=(
  alfred
  appcleaner
  cyberduck
  dropbox
  firefox
  goofy
  google-photos-backup
  hipchat
  iterm2
  macid
  paw
  rescuetime
  sequel-pro
  skype
  spotify
  sublime-text
  teamviewer
  utorrent
  vlc
  whatpulse
  google-chrome
  chefdk
)

brew cask install --appdir="/Applications" ${apps[@]}
brew cleanup
